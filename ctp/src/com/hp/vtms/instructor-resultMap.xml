<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"

        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="instructor">

    <resultMap type="com.hp.vtms.model.Instructor" id="instructorResultMap">
        <id property="inUserId" column="in_user_id"/>
        <result property="inUsername" column="in_username"/>
        <result property="inPassword" column="in_password"/>
        <result property="eventId" column="event_id"/>
        <result property="inAppName" column="in_app_name"/>
        <result property="inAllowControl" column="in_allow_control"/>
        <result property="inFrame" column="in_fname"/>
        <result property="inLastName" column="in_lname"/>
    </resultMap>

</mapper>

